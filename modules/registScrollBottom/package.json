{
  "name": "bLazyload",
  "version": "1.0.0",
  "description": "a simple&fast lazyload event module with smooth scroll",
  "main": "src/lazyload.js",
  "scripts": {
    "build": "babel --plugins transform-es2015-modules-commonjs ./src/lazyload.js --out-file ./dist/lazyload-commonjs.js; babel --plugins transform-es2015-modules-amd ./src/lazyload.js --out-file ./dist/lazyload-amd.js;babel --plugins transform-es2015-modules-umd ./src/lazyload.js --out-file ./dist/lazyload-umd.js",
    "sample": "open ./sample/index.html"
  },
  "keywords": [
    "lazyload"
  ],
  "author": "babyzone2004",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint-loader": "^1.6.0",
    "jshint-loader": "^0.8.3",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "observe-scroll-stop": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babyzone2004/babyzone2004.github.com/tree/master/modules/lazyload"
  }
}
